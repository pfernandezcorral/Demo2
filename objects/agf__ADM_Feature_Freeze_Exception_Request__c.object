<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>agf__Code_Coverage_Percentage_0_to_100</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
    agf__Code_Coverage_Percentage__c &gt; 1.0,
    agf__Code_Coverage_Percentage__c  &lt; 0.0
)</errorConditionFormula>
        <errorDisplayField>agf__Code_Coverage_Percentage__c</errorDisplayField>
        <errorMessage>Percentage must be between 0 and 100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>agf__Test_Case_Pass_Percentage_0_to_100</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
     agf__Test_Cases_Passing_Percentage__c  &gt; 1.0,
     agf__Test_Cases_Passing_Percentage__c  &lt; 0.0
)</errorConditionFormula>
        <errorDisplayField>agf__Test_Cases_Passing_Percentage__c</errorDisplayField>
        <errorMessage>Percentage must be between 0 and 100</errorMessage>
    </validationRules>
</CustomObject>
